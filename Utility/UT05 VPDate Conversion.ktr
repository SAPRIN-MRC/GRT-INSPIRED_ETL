    <jsScripts>
      <jsScript>
        <jsScript_type>0</jsScript_type>
        <jsScript_name>Script 1</jsScript_name>
        <jsScript_script>//Script here

var dateStr=upper(prior_covid_dx_date);
var yrPart=substr(dateStr,0,4);
var moPart=substr(dateStr,5,2);
var dyPart=substr(dateStr,8,2);
var yr=0;
var mo=0;
var dy=0;
//Process year
if(!isNum(yrPart)) {
  //replace last two digits with random value
  var century=str2num(substr(yrPart,0,2));
  if(century&lt;20) {
    yr=floor(rYear*100);
  } else {
    yr=floor(rYear*21);
  }
  yr=(century*100)+yr;
} else {
  yr=str2num(yrPart);
}
//Process month
if(!isNum(moPart)) {
  if("S1".equals(moPart)){
    mo=floor(rMonth*6);
  }
  if("S2".equals(moPart)){
    mo=floor(rMonth*6)+6;
  }
  if("Q1".equals(moPart)){
    mo=floor(rMonth*3);
  }
  if("Q2".equals(moPart)){
    mo=floor(rMonth*3)+3;
  }
  if("Q3".equals(moPart)){
    mo=floor(rMonth*3)+6;
  }
  if("Q4".equals(moPart)){
    mo=floor(rMonth*3)+9;
  }
  if("UU".equals(moPart)){
    mo=trunc(rMonth*12);
  }

} else {
  mo=str2num(moPart)-1;
}
// Process day
if(!isNum(dyPart)){
  if("UU".equals(dyPart)) {
    dy=floor(rDay*28)+1;
  }
  if("W1".equals(dyPart)) {
    dy=floor(rDay*7)+1;
  }
  if("W2".equals(dyPart)) {
    dy=floor(rDay*7)+8;
  }
  if("W3".equals(dyPart)) {
    dy=floor(rDay*7)+15;
  }
  if("W4".equals(dyPart)) {
    dy=floor(rDay*7)+22;
  }
}  else {
  dy=str2num(dyPart);
}
var date=new Date(yr,mo,dy);</jsScript_script>
      </jsScript>
      <jsScript>
