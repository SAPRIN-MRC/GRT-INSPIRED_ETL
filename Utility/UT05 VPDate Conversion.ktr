        <jsScript_script>// Perform the substring function
// 
// Usage:
// substr(var, from);
// substr(var, from, to);
//
// 2007-01-25
//
var str1= "Hello Pentaho!";
var str2= substr(str1, 6);
var str3= substr(str1, 6, 7);
Alert("Input : " + str1);
Alert("From position 6: " + str2);
Alert("From position 6 for 7 long : " + str3);
</jsScript_script>
      </jsScript>
      <jsScript>
        <jsScript_type>-1</jsScript_type>
        <jsScript_name>str2num_Sample</jsScript_name>
        <jsScript_script>// Converts the given String to a numeric Value.
//
// Usage:
// str2num(var);
// 1: String - The Variable with the String to convert.
//
// str2num(var, var);
// 1: String - The Variable with the String to convert.
// 2: String - The Format:
// 
// 2006-11-15
//
var str1 = "1.234,56";
var str2 = "12";
Alert((str2num(str1,"#,##0.00")));
Alert((str2num(str2)));</jsScript_script>
      </jsScript>
